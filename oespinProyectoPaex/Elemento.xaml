<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="oespinProyectoPaex.Elemento">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="ACTUALIZAR - ELIMINAR" FontSize="24" HorizontalTextAlignment="Center"></Label>
            <Entry x:Name="txtID" ></Entry>
            <Entry x:Name="txtNombre" ></Entry>
            <Entry x:Name="txtUsuario" ></Entry>
            <Entry x:Name="txtContrasena" ></Entry>

            <Button x:Name="btnActualizar" Text="Actualizar" Clicked="btnActualizar_Clicked"></Button>
            <Button x:Name="btnEliminar" Text="ELiminar" Clicked="btnEliminar_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>